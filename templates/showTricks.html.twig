
{% extends 'base.html.twig' %}


{% block title %}{{ parent() }} | Home{% endblock %}

{% block content %}
    <h1>Test</h1>
    {% for trick in tricks %}
        <div class="row">
            <div>
                <img src="{{ asset(trick.fileName ~ trick.ext) }}" />
            </div>
            <h4>{{ trick.name }}</h4>
            <p>{{ trick.description }}</p>
            <h6>{{ trick.grp }}</h6>

        </div>

    {% endfor %}
{% endblock %}